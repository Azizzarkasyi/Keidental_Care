<!-- rounded_corner_background.xml -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid
        android:color="@color/white"/>

    <!-- Corner radius -->
    <corners android:radius="8dp" />

    <!-- Stroke -->
    <stroke
        android:width="1dp"
        android:color="#9E9B9B" />
</shape>
