<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:padding="20dp"
        android:background="@drawable/bacgound"
        android:layout_height="match_parent"
        tools:context=".home_feature.Dokter">
    <androidx.cardview.widget.CardView
            android:background="@drawable/edit_background"
            android:id="@+id/cardview133"
            app:cardCornerRadius="10dp"
            android:layout_marginTop="50dp"
            android:layout_centerHorizontal="true"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">
        <RelativeLayout

                android:padding="@dimen/_10sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:layout_editor_absoluteX="56dp"
                app:layout_constraintTop_toTopOf="parent"
                android:id="@+id/relativeLayout2">


            <TextView
                    android:layout_centerHorizontal="true"
                    android:text="Detail Dokter"
                    android:textSize="@dimen/_16sdp"
                    android:layout_centerVertical="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView2"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
    <RelativeLayout
            android:layout_marginTop="40dp"
            android:padding="20dp"
            android:background="@drawable/bgcardhome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/relativeLayout4"
            tools:layout_editor_absoluteX="24dp"
            app:layout_constraintTop_toBottomOf="@+id/cardview133">

        <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:srcCompat="@drawable/dokter_cycle"
                android:id="@+id/imageViewdokter"/>
        <TextView
                android:layout_marginStart="20dp"
                android:textSize="16dp"
                android:text="Drg.  Rima Yunita.R"
                android:fontFamily="@font/inter_medium"
                android:layout_toEndOf="@+id/imageViewdokter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"/>
        <TextView
                android:textSize="15dp"
                android:layout_marginStart="20dp"
                android:text="Dokter GIgi Umum"
                android:textColor="#9E9B9B"
                android:fontFamily="@font/inter_medium"
                android:layout_below="@+id/textView6"
                android:layout_toEndOf="@+id/imageViewdokter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView7"/>
    </RelativeLayout>
    <RelativeLayout
            android:layout_marginTop="20dp"
            android:paddingBottom="40dp"
            android:background="@drawable/bgcardhome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.088"
            app:layout_constraintHorizontal_bias="0.4">

        <TextView
                android:fontFamily="@font/inter_bold"
                android:textSize="15dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:text="Info Praktek"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView8"/>
        <View
                android:id="@+id/sparator"
                android:layout_below="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/black"/>
        <TextView
                android:fontFamily="@font/inter_bold"
                android:textSize="12dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:layout_below="@+id/sparator"
                android:text="Kei Dental Care"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView9"/>
        <TextView
                android:fontFamily="@font/inter"
                android:textSize="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:text="Klinik Gigi, Kota Pariaman, Padang"
                android:layout_below="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView10"/>
        <TextView
                android:fontFamily="@font/inter"
                android:textSize="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:text="Sumatera Barat"
                android:layout_below="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView11"/>
        <TextView
                android:fontFamily="@font/inter_bold"
                android:textSize="12dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:text="Jadwal Praktek"
                android:layout_below="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView12"/>
        <TextView
                android:fontFamily="@font/inter"
                android:textSize="14dp"
                android:layout_marginTop="20dp"
                android:layout_marginStart="20dp"
                android:text="SENIN s\d SABTU 16.00 s\d 21.00 WIB"
                android:layout_below="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView13"/>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>